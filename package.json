{
  "name": "@tb-dev/auto-import-config",
  "version": "9.2.6",
  "description": "Config for unplugin-auto-import",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@10.12.1",
  "funding": "https://github.com/sponsors/ferreira-tb",
  "homepage": "https://github.com/ferreira-tb/auto-import-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreira-tb/auto-import-config.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/auto-import-config/issues"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --write",
    "lint": "eslint . --config eslint.config.js --cache",
    "lint-fix": "eslint . --config eslint.config.js --fix",
    "release": "pnpm run build && pnpm publish",
    "type-check": "tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "unplugin-auto-import": "^19.3.0",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^7.1.8",
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.17"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^2.5.0",
    "@tb-dev/utils": "^7.0.4",
    "@vueuse/core": "^13.4.0",
    "@vueuse/math": "^13.4.0",
    "@vueuse/router": "^13.4.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
}

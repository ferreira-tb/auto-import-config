{
  "name": "@tb-dev/auto-import-config",
  "version": "2.3.0",
  "description": "Config for unplugin-auto-import",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@9.5.0",
  "homepage": "https://github.com/ferreira-tb/auto-import-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreira-tb/auto-import-config.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/auto-import-config/issues"
  },
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "lint-fix": "eslint . --config eslint.config.js --fix",
    "prepare": "husky",
    "release": "pnpm run build && pnpm publish",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.3.3"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^3.8.8",
    "@types/node": "^20.14.10",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite-plugin-dts": "^3.9.1",
    "vue": "^3.4.31"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^2.0.0-beta.14",
    "@tb-dev/utility-types": "^1.4.4",
    "@tb-dev/utils": "^1.8.6",
    "@vueuse/core": "^10.11.0",
    "@vueuse/math": "^10.11.0",
    "@vueuse/router": "^10.11.0",
    "manatsu": "^0.13.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}

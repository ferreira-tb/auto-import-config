{
  "name": "@tb-dev/auto-import-config",
  "version": "9.0.3",
  "description": "Config for unplugin-auto-import",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@10.0.0",
  "homepage": "https://github.com/ferreira-tb/auto-import-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreira-tb/auto-import-config.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/auto-import-config/issues"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "lint-fix": "eslint . --config eslint.config.js --fix",
    "release": "pnpm run build && pnpm publish",
    "type-check": "tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "unplugin-auto-import": "^19.0.0",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^6.2.2",
    "@types/node": "^22.10.6",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite-plugin-dts": "^4.5.0",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^2.1.1",
    "@tb-dev/utils": "^4.1.3",
    "@vueuse/core": "^12.3.0",
    "@vueuse/math": "^12.3.0",
    "@vueuse/router": "^12.3.0",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}